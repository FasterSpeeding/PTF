{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["standard", "plugin:prettier/recommended"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["prettier", "@typescript-eslint"],
    "rules": {
        "prettier/prettier": "error"
    }
}
