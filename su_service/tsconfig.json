{
    "compilerOptions": {
        "outDir": "./dist",
        "module": "commonjs",
        "target": "es2020",
        "lib": ["es2020"],
        "alwaysStrict": true,
        "strict": true,
        "preserveConstEnums": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "esModuleInterop": true,
        "types": ["@cloudflare/workers-types", "@types/node"]
    },
    "include": ["src/*.ts", "src/**/*.ts"]
}
