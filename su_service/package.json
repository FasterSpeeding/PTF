{
    "name": "su-service",
    "version": "1.0.0",
    "description": "SU service cloudflare service.",
    "scripts": {
        "build": "webpack",
        "format": "prettier --write src/**/*.{ts,js,json} && prettier --write *.{json,md,js}",
        "lint": "eslint src/**/*.{ts,js,json} webpack.config.js --fix",
        "publish": "wrangler publish",
        "watch": "wrangler preview --watch"
    },
    "author": "Lucina",
    "license": "BSD-3-Clause",
    "repository": {
        "type": "git",
        "url": "https://github.com/FasterSpeeding/PTF"
